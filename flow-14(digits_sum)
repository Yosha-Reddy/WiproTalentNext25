import java.util.*;
class digitsum
{
    public static void main(String[] args)
    {
        int i,n,r,sum=0;
        Scanner sc=new Scanner(System.in);
        System.out.println("enter the number:");
        n=sc.nextInt();
        for(i=1;i!=0;i++)
        {
            r=n%10;
            sum=sum+r;
            n=n/10;
        }
        System.out.println("sumof digits is:"+sum);
    }
}
