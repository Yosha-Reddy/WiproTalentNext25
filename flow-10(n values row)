import java.util.*;
class rownumber
{
    public static void main(String[]args)
    {
        int n,i;
        Scanner sc=new Scanner(System.in);
        System.out.println("enter the value of n:");
        n=sc.nextInt();
        for(i=1;i<=n;i++)
        {
            System.out.print(i+" ");
        }
    }
}
