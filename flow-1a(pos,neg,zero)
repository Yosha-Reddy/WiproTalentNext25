import java.util.*;
class values{
    public static void main(String[] args)
    {
        int n;
        Scanner sc=new Scanner(System.in);
        System.out.println("enter the number");
        n=sc.nextInt();
        if(n>0)
        {
            System.out.println("positiv num");
        }
        else if(n<0)
        {
            System.out.println("negative num");
        }
        else
        {
            System.out.println("zero");
        }
    }
}
