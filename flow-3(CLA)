import java.util.*;
class Argu
{
    public static void main(String[] args)
    {
        if(args.length==0)
        {
            System.out.println("No values");
        }
        else{
            System.out.println("Values provided");
            for(String args1:args)
            {
                System.out.print(args1+",");
            }
        }
    }
}
