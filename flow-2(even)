import java.util.*;
class even
{
    public static void main(String[]args)
    {
        int n;
        Scanner sc=new Scanner(System.in);
        System.out.println("eneter the num:");
        n=sc.nextInt();
        if(n%2==0)
        {
            System.out.println("even");
        }
        else{
            System.out.println("odd");
        }
    }
}
